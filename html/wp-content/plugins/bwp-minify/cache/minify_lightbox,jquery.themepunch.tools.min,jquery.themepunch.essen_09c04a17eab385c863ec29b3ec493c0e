function getLatinCharsSet(){var lower="abcdefghijklmnopqrstuvwxyz";var upper=lower.toUpperCase();var azAZ=new Set(lower+upper);return azAZ;}
function allowThouSep(currentString,Char){if(currentString.length==0)
